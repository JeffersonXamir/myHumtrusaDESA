package com.humtrusa.entities;

import java.util.List;

import org.hibernate.Session;

import com.humtrusa.Sessionfactory.HibernateSessionFactory;



public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Session session  =null;
		session = HibernateSessionFactory.getSession();
		
		List<Alumnos> a = session.createQuery(" from Alumnos a ").list(); 
		
		for (Alumnos alumnos : a) {
			System.out.println(alumnos.getApellidos());
		}
	}


}
